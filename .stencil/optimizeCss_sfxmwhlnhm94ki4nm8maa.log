:host {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  border: 3px solid green;
}

article {
  height: 100vh;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
}
article::-webkit-scrollbar {
  width: 0 !important;
}
article {
  overflow: -moz-scrollbars-none;
  -ms-overflow-style: none;
}